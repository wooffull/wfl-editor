{
  "name": "wfl-editor",
  "version": "0.0.0",
  "description": "Game editor for the WFL engine.",
  "main": "main.js",
  "scripts": {
    "start": "mkdirp ./projects && electron ./src/main.js",
    "pretest": "jshint ./src",
    "test": "echo \"Tests Complete\""
  },
  "jshintConfig": {
    "esversion": 6,
    "node": true,
    "loopfunc": true,
    "predef": [
      "wfl",
      "window",
      "document",
      "Image"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wooffull/wfl-editor.git"
  },
  "author": "Hector Fernando Pineiro II",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wooffull/wfl-editor/issues"
  },
  "homepage": "https://github.com/wooffull/wfl-editor#readme",
  "dependencies": {
    "electron-prebuilt": "^1.4.1",
    "mkdirp": "^0.5.1",
    "watch": "^1.0.2"
  },
  "devDependencies": {
    "jshint": "^2.9.2"
  }
}
